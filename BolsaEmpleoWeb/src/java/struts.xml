<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="false" />
    <constant name="struts.configuration.xml.reload" value="true" />
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        
        <result-types>
            <result-type name="fotoSolicitante" class="Actions.CustomImageResult" />
        </result-types>
        <action name="getImagen" class="Actions.ImagenUtilAction">
            <interceptor-ref name="defaultStack">
                <param name="maximumSize">2097152</param>
                <param name="allowedTypes">
                    image/png,image/gif,image/jpeg,image/pjpeg,image/jpg
                </param>
            </interceptor-ref>
            <result name="success" type="fotoSolicitante" />
            <result name="input" type="fotoSolicitante" />
        </action>
        
        <action name="menuPrincipal">
            <result name="input" type="dispatcher">/recursos/usuarios/menuPrincipal.jsp</result>
        </action>
        <action name="iniciarSesionInit" class="Actions.IniciarSesionAction">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result name="input" type="dispatcher">/recursos/usuarios/login.jsp</result>
        </action>
        <action name="iniciarSesion" method="iniciarSesionUsuario" class="Actions.IniciarSesionAction">
            <result name="input" type="dispatcher">/recursos/usuarios/login.jsp</result>
            <result name="error" type="dispatcher">/recursos/usuarios/login.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
        
        <action name="insertarSolicitante" class="Actions.InsertarSolicitanteAction">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result name="input" type="dispatcher">/recursos/usuarios/insertarSolicitante.jsp</result>
        </action>
        <action name="insertarSolicitanteProcess" method="insertar" class="Actions.InsertarSolicitanteAction">
            <result name="input" type="dispatcher">/recursos/usuarios/insertarSolicitante.jsp</result>
            <result name="error" type="dispatcher">/recursos/usuarios/insertarSolicitante.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        <action  class="Actions.BuscarSolicitantesAction" name="buscarSolicitante">
            <result name="input" type="dispatcher">/recursos/usuarios/buscarSolicitantes.jsp</result>
        </action>
        <action  class="Actions.BuscarSolicitantesAction" name="buscarSolicitantesProcess" method="buscar">
            <result name="buscarSolicitantes" type="dispatcher">/recursos/usuarios/mantenerSolicitantes.jsp</result>
        </action>
        <action class="Actions.EditarSolicitanteAction" name="editarSolicitante">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result type="dispatcher" name="input">/recursos/usuarios/editarSolicitante.jsp</result>
        </action>
        
        <action class="Actions.EditarSolicitanteAction" name="editarSolicitanteProcess" method="editar">
            <result type="dispatcher" name="input">/recursos/usuarios/editarSolicitante.jsp</result>
            <result type="dispatcher" name="error">/recursos/usuarios/editarSolicitante.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        <action class="Actions.EliminarSolicitanteAction" name="eliminarSolicitante">
            <result type="dispatcher" name="input">/recursos/usuarios/eliminarSolicitante.jsp</result>
        </action>
        
        <action class="Actions.EliminarSolicitanteAction" name="eliminarSolicitanteProcess" method="eliminar">
            <result type="dispatcher" name="input">/recursos/usuarios/eliminarSolicitante.jsp</result>
            <result type="dispatcher" name="error">/recursos/usuarios/eliminarSolicitante.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
        
        
        
        <action name="iniciarSesionEmpleadorInit" class="Actions.IniciarSesionEmpleadorAction">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result name="input" type="dispatcher">/recursos/empresa/login.jsp</result>
        </action>
        <action name="iniciarSesionEmpleador" method="iniciarSesionEmpleador" class="Actions.IniciarSesionEmpleadorAction">
            <result name="input" type="dispatcher">/recursos/empresa/login.jsp</result>
            <result name="error" type="dispatcher">/recursos/empresa/login.jsp</result>
            <result name="success" type="dispatcher">/recursos/empresa/editarEmpleador.jsp</result>
        </action>
        
        <action class="Actions.EditarEmpleadorAction" name="editarEmpleador">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result type="dispatcher" name="input">/recursos/empresa/editarEmpleador.jsp</result>
        </action>
        <action class="Actions.EditarEmpleadorAction" name="editarEmpleadorProcess" method="editar">
            <result type="dispatcher" name="input">/recursos/empresa/editarEmpleador.jsp</result>
            <result type="dispatcher" name="error">/recursos/empresa/editarEmpleador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
        <action name="registrarEmpleador" class="Actions.InsertarEmpleadoresAction">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result name="input" type="dispatcher">/recursos/empresa/registrarEmpleador.jsp</result>
        </action>
        <action name="registrarEmpleadorProcess" method="insertar" class="Actions.InsertarEmpleadoresAction">
            <result name="input" type="dispatcher">/recursos/empresa/registrarEmpleador.jsp</result>
            <result name="error" type="dispatcher">/recursos/empresa/registrarEmpleador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
        <action class="Actions.EliminarEmpleadorAction" name="eliminarEmpleador">
            <result type="dispatcher" name="input">/recursos/empresa/eliminarEmpleador.jsp</result>
        </action>
        
        <action class="Actions.EliminarEmpleadorAction" name="eliminarEmpleadorProcess" method="eliminar">
            <result type="dispatcher" name="input">/recursos/empresa/eliminarEmpleador.jsp</result>
            <result type="dispatcher" name="error">/recursos/empresa/eliminarEmpleador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
        
        
        
        <action name="insertarAdministrador" class="Actions.InsertarAdministradorAction">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result name="input" type="dispatcher">/recursos/usuarios/insertarAdministrador.jsp</result>
        </action>
        <action name="insertarAdministradorProcess" method="insertar" class="Actions.InsertarAdministradorAction">
            <result name="input" type="dispatcher">/recursos/usuarios/insertarAdministrador.jsp</result>
            <result name="error" type="dispatcher">/recursos/usuarios/insertarAdministrador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        <action  class="Actions.BuscarAdministradoresAction" name="buscarAdministrador">
            <result name="input" type="dispatcher">/recursos/usuarios/buscarAdministradores.jsp</result>
        </action>
        <action  class="Actions.BuscarAdministradoresAction" name="buscarAdministradoresProcess" method="buscar">
            <result name="buscarAdministradores" type="dispatcher">/recursos/usuarios/mantenerAdministradores.jsp</result>
        </action>
        <action class="Actions.EditarAdministradorAction" name="editarAdministrador">
            <interceptor-ref name="defaultStack">
                <param name="validation.excludeMethods">execute</param>
            </interceptor-ref>
            <result type="dispatcher" name="input">/recursos/usuarios/editarAdministrador.jsp</result>
        </action>
        
        <action class="Actions.EditarAdministradorAction" name="editarAdministradorProcess" method="editar">
            <result type="dispatcher" name="input">/recursos/usuarios/editarAdministrador.jsp</result>
            <result type="dispatcher" name="error">/recursos/usuarios/editarAdministrador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        <action class="Actions.EliminarAdministradorAction" name="eliminarAdministrador">
            <result type="dispatcher" name="input">/recursos/usuarios/eliminarAdministrador.jsp</result>
        </action>
        
        <action class="Actions.EliminarAdministradorAction" name="eliminarAdministradorProcess" method="eliminar">
            <result type="dispatcher" name="input">/recursos/usuarios/eliminarAdministrador.jsp</result>
            <result type="dispatcher" name="error">/recursos/usuarios/eliminarAdministrador.jsp</result>
            <result name="success" type="redirect">/recursos/pantallaPrincipal.jsp</result>
        </action>
        
    </package>
    
</struts>
